<?xml version='1.0' encoding='utf-8'?>
<widget id="com.dkarzon.beerwhere" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <preference name="loglevel" value="DEBUG" />
    <name>BeerWhere</name>
    <description>Sample Cordova app with Visual Studio tools and Ionic</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">dkarzon</author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="SplashScreen" value="screen" />
    <preference name="windows-target-version" value="8.1" />
    <vs:features>
        <vs:feature>org.apache.cordova.geolocation@0.3.8</vs:feature>
    </vs:features>
    <vs:platformSpecificValues>
        <vs:platformSpecificWidget platformName="windows8" />
    </vs:platformSpecificValues>
</widget>
