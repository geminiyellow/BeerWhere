<?xml version="1.0" encoding="utf-8"?>
<widget xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps" id="com.dkarzon.beerwhere" version="1.0.0.0" xmlns="http://www.w3.org/ns/widgets">
  <name>BeerWhere</name>
  <description>Sample Cordova app with Visual Studio tools and Ionic</description>
  <author href="http://cordova.io" email="dev@cordova.apache.org">dkarzon</author>
  <content src="index.html" />
  <access origin="*" />
  <preference name="SplashScreen" value="screen" />
  <preference name="windows-target-version" value="8.1" />
  <vs:features>
    <vs:feature>org.apache.cordova.geolocation@0.3.8</vs:feature>
  </vs:features>
  <vs:platformSpecificValues>
    <vs:platformSpecificWidget platformName="windows8" />
  </vs:platformSpecificValues>
</widget>